{
  "name": "allinone",
  "chef_type": "environment",
  "ipaddress": "192.168.1.2",
  "override_attributes": {
     "rabbitmq": {
       "version": "3.4.3-1",
       "default_user": "flg-cloud",
       "default_pass": "flg-cloud",
       "enabled_users": [ {
         "name": "flg-cloud",
         "password": "flg-cloud",
         "rights": {
           "vhost": "",
           "conf": ".*",
           "write": ".*",
           "read": ".*"
         } 
       } ]
     },
     "mysql": {
       "allow_remote_root": true,
       "root_network_acl": ["192.168.1.0/24"]	
     },
     "openstack": {
      "endpoints": {
        "host": "192.168.1.2",
        "bind-host": "192.168.1.2",
        "db": {
          "host": "192.168.1.2"
        },
        "mq": {
          "host": "192.168.1.2"
        }
      },
      "mq": {
        "user": "admin"
      },
      "compute": {
        "network": {
          "service_type": "neutron"
        },
        "scheduler": {
          "default_filters": ["AvailabilityZoneFilter", "RamFilter", "ComputeFilter", "ComputeCapabilitiesFilter", "ImagePropertiesFilter", "ServerGroupAntiAffinityFilter", "ServerGroupAffinityFilter"]
         } 
      },
      "network": {
        "debug": "True",
        "verbose": "True",
        "allow_overlapping_ips": "True",
        "service_plugins": ["neutron.services.l3_router.l3_router_plugin.L3RouterPlugin"],
        "misc_neutron": ["[ovs]", "local_ip = 192.168.1.2", "", "[agent]", "tunnel_types = vxlan"],
        "ml2": {
          "tenant_network_types": "vxlan",
          "mechanism_drivers": "openvswitch",
          "vni_ranges": "1001:2000"
        },
        "l3": {
          "gateway_external_network_name": "external"
        },
        "metadata": {
          "nova_metadata_ip": "192.168.1.2"
        }
      },
      "block-storage": {
        "volume": {
          "create_volume_group": true,
          "default_volume_type": "lvm"
         }
      },
      "bare-metal": {
        "user": "andesm",
        "group": "andesm"
      }
    }
  }
}

